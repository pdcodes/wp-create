{
  "name": "bluetext/PROJECTNAME",
  "authors": [
    {
      "name": "Bluetext",
      "email": "technology@bluetext.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:pantheon-systems/WordPress.git"
    }
  ],
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "minimum-stability": "dev",
  "require": {
    "wpackagist-plugin/intuitive-custom-post-order": "3.1.*",
    "wpackagist-plugin/wordpress-seo": "7.6.*",
    "wpackagist-plugin/wp-help": "1.5.*",
    "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "2.0.*",
    "wpackagist-plugin/minqueue": "1.1.*",
    "ahhacreative/ahha_plugin_gravityforms": "2.3.2.2",
    "ahhacreative/ahha_plugin_acf": "5.6.10"
  }
}